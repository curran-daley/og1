set loc1 to {492, 22}
set loc2 to {907, 22}
set loc3 to {367, 22}
set loc4 to {872, 22}

repeat 1000 times
	tell application "System Events"
		delay 30
		activate application "Brave Browser Beta"
		delay 30
		activate application "Brave Browser"
		delay 30
		repeat 10 times
			click at loc1
			delay 30
			click at loc1
			delay 30
			click at loc2
			delay 30
			click at loc2
			delay 30
			click at loc3
			delay 30
			click at loc3
			delay 30
			click at loc4
			delay 30
			click at loc4
			delay 30
		end repeat
		delay 30
		quit application "Brave Browser Beta"
		delay 30
		quit application "Brave Browser Browser"
		delay 30
	end tell
end repeat

